{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run God Mode Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "preLaunchTask": "webpack: watch",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "postDebugTask": "terminate webpack watch"
    }
  ],
  "inputs": [],
  "compounds": []
}
